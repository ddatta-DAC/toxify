apt-get install gfortran
    2  apt-get install software-properties-common
    3  add-apt-repository ppa:webupd8team/java
    4  apt-get install oracle-java8-installer
    5  wget https://cran.cnr.berkeley.edu/src/base/R-3/R-3.2.3.tar.gz
    6  cd R-3.2.3.tar.gz
    7  tar xvzf R-3.2.3.tar.gz
    8  cd R-3.2.3
    9  ./configure --with-readline=no --with-x=no
   10  make
   11  history

   apt-get install -y build-essential python python-dev python-pip
   apt-get install pkg-config libnlopt-dev
   apt-get install libcurl4-gnutls-dev librtmp-dev
   pip install pycurl
   apt-get install git-lfs
   git-lfs clone https://github.com/rgacesa/ToxClassifier.git


